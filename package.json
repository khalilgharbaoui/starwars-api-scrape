{
  "name": "react-bootstrap-boilerplate",
  "version": "0.0.2",
  "description": "A Webpack boilerplate with hot reloading React components, and error handling on module and component level. with bootstrap, jquery, react-router, easy-transitions. and deploy bash.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Khalil Gharbaoui <kaygeee@gmail.com> (http://github.com/in/khalilgharbaoui)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/khalilgharbaoui/empty-react-boilerplate/issues"
  },
  "homepage": "https://github.com/khalilgharbaoui/empty-react-boilerplate",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react-hmre": "^1.0.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "jquery": "^2.2.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1"
  }
}
